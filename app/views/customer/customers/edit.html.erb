<h1>edit</h1>
<!-- form_with部分 -->
<%= form_with model:@customer, local:true do |f| %>

<div>
    <div><h1>氏名</h1>
      <%= f.text_field :first_name %><%= f.text_field :last_name %>
    </div>

    <div>
      <h1>カナ</h1>
      <%= f.text_field :first_name_kana %><%= f.text_field :last_name_kana %>
    </div>

    <div>
      <h1>郵便番号</h1>
      <%= f.text_field :postal_code %>
    </div>

    <div>
      <h1>住所</h1>
      <%= f.text_field :address %>
    </div>

    <div>
      <h1>電話番号</h1>
      <%= f.text_field :telephone_number %>
    </div>
    <div>
      <h1>メールアドレス</h1>
      <%= f.text_field :email %>
    </div>

    <div>
     <%= f.submit "編集内容を保存する" %>
    <%= link_to '退会する', customers_quit_path, class: "glyphicon glyphicon-tint btn btn-danger"%>
    </div>
</div>



<% end %>